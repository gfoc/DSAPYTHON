//Program to implement insertion sort 
def insertion_sort(list_a):
    for i in range(1,len(list_a)):
        c=i
        while c>0 and list_a[c]<list_a[c-1]:
            a = list_a[c]
            list_a[c]=list_a[c-1]
            list_a[c-1]=a
            c=c-1
list_a = [33,22,55,99,11,100,101,10,9,5,7]
insertion_sort(list_a)
print(list_a)
